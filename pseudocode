 create a function of towerofhanoi with parameters torod,fromrod,auxrod,rods DO
  IF rods==0 then return
  ELSE 


#Linear search
 declare variable of INTEGER ARRAY arr of size size, tofind,c=0
 input values for both arr using loop and tofind
 using loop from i=0 till i<size DO
   IF arr[i]==tofind,then exit loop ,make c=1 and print"found"
increment i by 1
IF c==0 ,print "not found"


#Binary search
 declare integer variables mid,high,low,i ,tofind and a array arr of size size
 input arr of size size from user in ascending order sorted form
 low=arr[i]
 high=arr[size-1]
 using loop till low<=high
   mid=(low+high) /2
   IF arr[mid]==tofind then return mid
   ELSE IF tofind>mid, then low=mid+1 
   ELSE IF tofind<mid then high=mid-1 
print the mid as the index at which tofind is found

#Bubble sort
declare an integer array arr of size size
using loop from k=size-1, till k>=0 DO
 using loop from i=0 till i<n-1 DO
  IF arr[i] >arr[i+1], then temp=arr[i],arr[i]=arr[i+1],arr[i+1]=temp
 increment i by 1 
decrement j by 1

