#Sum of three numbers
declare the variables number1,number2,sum as INTEGER datatype
initialise sum=0
input values from user in number1, number2 variable.
set sum= number1+number2
display sum

#Sum of square of 2 numbers
declare num1,num2,sum,squareofnum1,squareofnum2 as INTEGER datatype
initialise sum=0
squareofnum1=num1*num1
squarofnum2=num2*num2
sum= squareofnum1+squareofnum2
display sum

#Reverse a number
 declare INTEGER variables i,number,reverseofnumber
 using loop start with reverseofnumber=0, till number>0 ,DO
   reverseofnumber=reverseofnumber*10 + number%10 
   number=number/10
display reverseofnumber

 #Reverse a string
 declare a character string array str,stringreverse of 'size'size
 using loop from i=0,j=size, till j>=0 DO
  stringreverse[i]= str[j]
  increment i by 1 and decrement j by 1 at each iteration
display stringreverse

#Find the given character in the string, return true or false
declare a variable str of size size string array datatype, a character variable tofind that is to be searched
input str and tofind from user
using loop from i=0 till i<size DO
 IF str[i]==tofind then  boolean=true
 ELSE  boolean=false
increment i by 1
print boolean

#Find the number of occurrences of a character in a given string
 declare a variable str of size size string array datatype, a character variable tocount whose frequency is to be counted
input str and tocount from user
initialise count =0
using loop from i=0 till i<size DO
 IF str[i]==tocount then count= count+1
increment i by 1 at each iteration
display "count of tocount is count"

